{
  "name": "queerboots",
  "version": "0.0.1",
  "description": "website for queer boots",
  "scripts": {
    "dev": "nodemon ./src/main.ts",
    "build": "npm run prep && tsc && node dist/main.js",
    "prep": "mkdir -p ./dist && touch ./dist/main.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:generate": "npm run typeorm -- migration:generate --config src/ormconfig.json --connection --name ",
    "migration:run": "npm run typeorm -- migration:run",
    "start": "NODE_ENV=development nodemon ./src/main.ts",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/queerboots/queerboots.git"
  },
  "author": "queer boots collective",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/queerboots/queerboots/issues"
  },
  "homepage": "https://github.com/queerboots/queerboots#readme",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.31"
  }
}
